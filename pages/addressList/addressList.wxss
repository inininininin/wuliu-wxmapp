/* pages/addressList/addressList.wxss */
page{
  background: #f2f2f2;
}
.addressItem{
  width: 710rpx;
  min-height: 160rpx;
  background: #ffffff;
  border-radius: 15rpx;
  margin: 20rpx 20rpx 0;
  line-height: 45rpx;
  font-size: 28rpx;
  box-sizing: border-box;
  padding: 20rpx 20rpx;
}
.lineStart{
  border-bottom: 1rpx solid #e5e5e5;
  width: 100%;
  padding-bottom: 15rpx;
  color: #333333;
}
.lineEnd{
  padding-top: 15rpx;
  color: #999999;
  font-size: 26rpx;
}