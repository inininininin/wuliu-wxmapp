/* pages/orderDetail/orderDetail.wxss */
page {
  background: #f5f5f5;
  padding-bottom: 20rpx;
  font-size: 28rpx;
}

.topMain {
  width: 750rpx;
  /* height: 730rpx; */
  background: #ffffff;
  margin-top: 20rpx;
  float: left;
  padding-bottom: 24rpx;
}

.topMain .type1 {
  width: 100%;
  /* height: 44rpx; */
  line-height: 44rpx;
  margin-top: 24rpx;
  float: left;
}

.topMain .type1 .faIcon {
  width: 35rpx;
  height: 44rpx;
  float: left;
  margin: 0 30rpx 0 25rpx;
}

.faIcon1 {
  width: 29rpx;
  height: 30rpx;
  margin: 7rpx 33rpx 0 29rpx;
  float: left;
}

.topMain .type1>view {
  width: 600rpx;
  /* height: 44rpx; */
  line-height: 44rpx;
  word-break: break-all;
  word-wrap: break-word;
  /* margin-top: 22rpx; */
  float: left;
}

.type2 {
  width: 100%;
  /* height: 40rpx; */
