/*五角星之间的间距*/
/*五角星*/
.star .icon:before {
  content: '\f005';
  font-family: FontAwesome;
  position: absolute;
  left: 0;
  top: 0;
  display: block;
  overflow: hidden;
}

.star .icon {
  display: block;
  font-size: 21px;
  text-align: center;
  width: 38rpx;
  height: 56rpx;
  line-height: 56rpx;
  position: relative;
  white-space: pre;
}

/*灰色五角星*/
.star .icon_gray {
  color: #aaa
}

/*黄色五角星*/
.star .icon_yellow:before {
  color: #FFD200;
}

