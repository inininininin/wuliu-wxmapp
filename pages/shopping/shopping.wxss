/* pages/shopping/shopping.wxss */
.container {
  box-sizing: border-box;
  /* height: 100%; */
  /* background: #fff; */
}

.header {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 999
}

.title-bar {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.searchThisEve{
  /* float: left; */
  display: inline-block;
  width:108rpx !important;
  height: 60rpx !important;
  line-height: 60rpx;
  border-radius: 25rpx;
  background: #1890ff;
  color: #fff;
  font-size: 28rpx;
  text-align: center;
  margin-left: 10rpx;
}
.searchBox {
  display: inline-block;
  width: 368rpx !important;
  height: 60rpx !important;
  background: #eeeeee;
  border-radius: 30rpx;
  margin-left: 24rpx;
  vertical-align: middle;
  line-height: 60rpx;
}

.searchBox>image {
  width: 28rpx;
  height: 30rpx;
  /* margin: 0 15rpx 0 23rpx; */
  margin: 15rpx 15rpx 15rpx 23rpx;
  float: left;
}

.searchBox>input {
  width: 250rpx;
  float: left;
  height: 60rpx;
  line-height: 60rpx;
  font-size: 28rpx;
  color: #333333;
  padding: 0 5rpx 0 6rpx;
  vertical-align: middle;
  float: left;
}
.searchBox>icon{
  width: 42rpx;
  /* margin-top: 15rpx; */
  padding: 15rpx 0;
  /* height:60rpx; */
  display: inline-block;
  float: left;
}

/* 滚动导航 */
.navbarList {
  width: 100%;
  height: 176rpx;
  overflow-x: scroll;
  background: #fff;
}

/* 二层次导航栏 */
.scroll-header {
  /* position: fixed; */
  height: 88rpx;
  font-size: 30rpx;
  /* width: 750rpx; */
  line-height: 88rpx;
  display: flex;
  white-space: nowrap;
}

.scroll-header .scroll-view-item{
  height: 88rpx !important;
  width: auto;
  padding: 0 20rpx;
  /* float: left; */
  /* border: 1px solid red; */
  display: inline-block;
  position: relative;
}

.navbar {
  position: fixed;
  height: 88rpx;
  width: 750rpx;
  border-bottom: 1rpx solid #e2e2e2;
  box-sizing: border-box;
}

.scroll-header .scroll-view-item.active {
  color: #FF932E !important;
  font-size: 30rpx;
  line-height: 88rpx;
  height: 88rpx;
}

.navbar>view {
  position: relative;
  width: 375rpx;
  text-align: center;
  line-height: 98rpx;
  height: 88rpx;
  font-size: 30rpx;
  color: #333;
  display: inline-block;
  flex: auto;
}

.scroll-header .scroll-view-item.active:after {
  content: "";
  display: block;
  position: absolute;
  top: 82rpx;
  border-radius: 3rpx;
  left:  calc(50% - 20rpx);
  width: 40rpx;
  height: 6rpx;
  background: #FF932E;
}

.scroll-header-this{
  height: 88rpx;
  font-size: 30rpx;
  /* width: 750rpx; */
  line-height: 88rpx;
  display: flex;
  white-space: nowrap;
  margin-top: 88rpx;
}
.scroll-header-this .scroll-view-item{
 
  height: 88rpx !important;
  width: auto;
  padding: 0 20rpx;
  display: inline-block;
  position: relative;
}
.scroll-header-this .scroll-view-item.active {
  color: #FF932E !important;
  font-size: 30rpx;
  line-height: 88rpx;
  height: 88rpx;
}

page {
  background: #f5f5f5;
}

.shopItem {
  margin-top: 20rpx;
  width: 750rpx;
  height: 296rpx;
  background: #ffffff;
  float: left;
}

.shopItem>image {
  width: 218rpx;
  height: 248rpx;
  margin: 24rpx 20rpx;
  float: left;
}

.shopItem .detail {
  width: 460rpx;
  height: 248rpx;
  float: left;
  margin-top: 24rpx;
}

.shopItem .detail .title {
  height: 42rpx;
  font-size: 30rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 42rpx;
  margin-bottom: 14rpx;
}

.shopItem .detail .address {
  height: 37rpx;
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
  line-height: 37px;
  margin-bottom: 8rpx;
  width: 100%;
}

.shopItem .detail .address text {
  float: left;
  line-height: 37rpx;
}

.shopItem .detail .address text:last-child {
  color: #333333;
}

.shopItem .detail .position>view:nth-child(1) {
  width: 78rpx;
  display: inline-block;
  float: left;
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #999999;
  line-height: 37rpx;
}

.shopItem .detail .position>view:nth-child(2) {
  width: 382rpx;
  height: 148rpx;
  display: inline-block;
  float: left;
}

.position>view {
  height: 148rpx;
  font-size: 26rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 37rpx;
}
.position>view>view>view{
  display: inline-block;
}
.position>view>view>view>text ,.colorEve{
  color: #ED2828;
}

.totalCount {
  font-size: 26rpx;
  color: #999999;
  padding: 20rpx 0;
  width: 100%;
  text-align: center;
  /* margin: 10rpx 0; */
}

.bgImg {
  width: 508rpx;
  height: 264rpx;
  opacity: 0.7;
  margin: 265rpx 121rpx 24rpx;
}

.loginTitle {
  width: 100%;
  height: 40rpx;
  font-size: 28rpx;
  font-family: PingFangSC, PingFangSC-Regular;
  font-weight: 400;
  text-align: left;
  color: #333333;
  line-height: 40rpx;
  text-align: center;
  display: block;
}

.gologinBtn {
  width: 490rpx;
  height: 80rpx;
  background: linear-gradient(90deg, #0e4090, #1890ff);
  border-radius: 50rpx;
  font-size: 38rpx;
  margin: 100rpx 130rpx 0;
  color: #ffffff;
  text-align: center;
  line-height: 80rpx;
}